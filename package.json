{
  "name": "react-market-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@openchannel/react-common-components": "^0.1.4",
    "@openchannel/react-common-services": "^0.1.4",
    "@types/react-redux": "^7.1.18",
    "http-proxy-middleware": "^2.0.1",
    "oidc-client": "^1.11.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "start:stage1": "env-cmd -f .env.stage1 react-scripts start",
    "start:dev1": "env-cmd -f .env.dev1 react-scripts start",
    "start:us1": "env-cmd -f .env.prod react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "build:stage1": "env-cmd -f .env.stage1 react-scripts build",
    "build:dev1": "env-cmd -f .env.dev1 react-scripts build",
    "build:us1": "env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,ms,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux": "^3.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.3",
    "redux-devtools-extension": "^2.13.9"
  }
}
